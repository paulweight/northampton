{
    "name": "northampton/website",
    "package": {
        "files": [
            "/CLIENT_VERSION",
            "/config/permissions/custom",
            "/jadu/**",
            "/public_html/**",
            "/upgrades/migrations/helpers/autoloader.php",
            "/var/**",
            "/vendor/**",
            "!/public_html/site/styles/widget/**",
            "!/vendor/jadu/meteor-ant/**"
        ],
        "version": "CLIENT_VERSION",
        "provider": "gdrive",
        "combine": {
            "jadu/cms": "16.7.0"
        }
    },
    "migrations": {
        "name": "Northampton Migrations",
        "table": "JaduMigrationsCustom",
        "namespace": "DoctrineMigrations",
        "directory": "upgrades/migrations"
    },
    "extensions": [
        "Meteor\\Package\\Provider\\GoogleDrive\\ServiceContainer\\GoogleDrivePackageProviderExtension"
    ],
    "autoload": {
        "composer": [
            "spacecraft/backups",
            "spacecraft/migrations"
        ]
    }
}
